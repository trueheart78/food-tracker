<div id='page'>
  <div id='header'>
    <h1 class='embiggen'>🍔 In The Kitchen 🍔</h1>
  </div>
  <%= erb :_navigation, locals: { data_files: @data_files } %>
  <div id='content'>
    <% @data_files.each do |data_file| %>
      <a id='<%= data_file.safe_name %>'></a>
      <p>
      <%= erb :_data_file, locals: { data_file: data_file } %>
      </p>
    <% end %>
  </div>
</div>
